@model IEnumerable<App.Models.PublicadorDTO>

@{
    ViewData["Title"] = "Publicadores Impedidos de Fazer Parte";
}

<h1>    
    😐 @ViewData["Title"]
</h1>

<div class='list-group list-group-flush mt-4'>
    @foreach (var item in Model)
    {
        <a class='list-group-item list-group-item-action' asp-action="Details" asp-route-id="@item.Id">
            🔴 @item.Nome
        </a>
        
    }
</div>
